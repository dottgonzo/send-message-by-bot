{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",

    "sourceMap": true,
    "strict": false,
    "noImplicitAny": true,

    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,

    "moduleResolution": "node",

    "esModuleInterop": true,

    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
